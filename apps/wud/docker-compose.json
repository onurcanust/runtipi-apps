{
  "schemaVersion": 2,
  "services": [
    {
      "name": "wud",
      "image": "getwud/wud:latest",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        { "key": "WUD_SERVER_PORT", "value": "3000" }
      ],
      "volumes": [
        {
          "hostPath": "/var/run/docker.sock",
          "containerPath": "/var/run/docker.sock",
          "readOnly": true
        }
      ]
    }
  ]
}
